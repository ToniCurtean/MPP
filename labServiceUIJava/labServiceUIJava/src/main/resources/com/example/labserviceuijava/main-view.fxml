<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.labserviceuijava.controller.MainController">
   <children>
      <ListView fx:id="concertsView" layoutX="14.0" layoutY="35.0" prefHeight="200.0" prefWidth="200.0" />
      <Label layoutX="14.0" layoutY="14.0" text="All Upcoming Concerts!" />
      <ListView fx:id="concertsDateView" layoutX="223.0" layoutY="35.0" prefHeight="200.0" prefWidth="200.0" />
      <TextField fx:id="dateText" layoutX="433.0" layoutY="67.0" />
      <Label layoutX="433.0" layoutY="35.0" text="Search concerts by date!" />
      <Button fx:id="searchButton" layoutX="433.0" layoutY="109.0" mnemonicParsing="false" onAction="#onSearchButtonClick" text="Search" />
      <Label layoutX="14.0" layoutY="253.0" text="Fill to buy!" />
      <Label layoutX="14.0" layoutY="271.0" text="Name" />
      <TextField fx:id="nameText" layoutX="14.0" layoutY="289.0" />
      <TextField fx:id="numberText" layoutX="171.0" layoutY="289.0" />
      <Label layoutX="171.0" layoutY="271.0" text="Number of seats" />
      <Button fx:id="buyButton" layoutX="269.0" layoutY="327.0" mnemonicParsing="false" onAction="#onBuyButtonClick" text="Buy" />
      <Button fx:id="logoutButton" layoutX="531.0" layoutY="361.0" mnemonicParsing="false" onAction="#onLogoutButtonClick" text="Log Out" />
   </children>
</AnchorPane>
